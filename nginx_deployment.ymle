apiVersion: apps/v1
kind: Deployment
metadata:
  name: model-deployment
  labels:
    app: model
spec:
  replicas: 1
  selector:
    matchLabels:
      app: model
  template:
    metadata:
      labels:
        app: model
    spec:
      containers:
      - name: model
        image: 015162308185.dkr.ecr.us-west-2.amazonaws.com/abalone:87e3e572-0edb-4169-95b3-6543b9232208
        ports:
        - containerPort: 8080
